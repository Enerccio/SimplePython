def test():
    x = iter([1, 2, 3, 4, 5])
    k = []
    y = iter(xrange(2))
    while True:
        for v in x:
            a = None
            try:
                a = y.next()
            except StopIteration:
                a = -1
            if a == -1:
                return k
            k.append(v * a)
            
print test()