import threading

class MyThread(threading.Thread):
    def __init__(self, x):
        super(MyThread, self).__init__("My new shiny thread " + x)
        
    def execute(self):
        for x in [1, 2, 3, 4]:
            print x, ". iteration of thread ", self.get_name()

def test():
    t = MyThread(5)
    print t.executed
    print getattr(t, "executed")
