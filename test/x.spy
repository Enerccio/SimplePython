"""
Some documentation for proxy
"""
class TestProxy(object):
    def __init__(self, proxy):
        self.proxy = proxy
        self.__doc__ = proxy.__doc__
        
    def run(self, x):
        try:
            print "inside proxy"
            return self.proxy.run(x)
        finally:
            print "outside proxy"

"""
Some documentation for class
"""
class Test(object):
    def __init__(self):
        return TestProxy(self)
    
    def run(self, x):
        print "x = ", x
        return "ok"

def test():
    x = Test()
    print x.run(10)
    print x.__doc__