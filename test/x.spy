def dosomething(x, y):
    return x + y

def test():
    x = xrange(256)
    y = xrange(256)
    
    xi = iter(x)
    try:
      while True:
        i = xi.next()
        yi = iter(y)
        try:
          while True:
            j = yi.next()
            dosomething(i, j)
        except StopIteration as e:
            pass
    except StopIteration as e:
        pass